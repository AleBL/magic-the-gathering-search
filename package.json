{
  "name": "vite-reactts-electron-starter",
  "version": "0.5.0",
  "license": "MIT",
  "main": "dist-electron/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "vite-reactts-electron-starter",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist dist-electron dist-vite",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "deps:update": "taze major -I"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.9",
    "@vitejs/plugin-react": "^4.3.1",
    "bootstrap": "^5.3.3",
    "i18next": "^23.11.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-icons": "^5.2.1",
    "vite": "^5.3.2",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.34.3",
    "postcss": "^8.4.39",
    "prettier": "2.6.0",
    "tailwindcss": "^3.4.4",
    "taze": "^0.14.0",
    "typescript": "^5.5.3"
  },
  "build": {
    "asar": true,
    "files": [
      "dist-electron",
      "dist-vite"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
